{
	"parser": "@typescript-eslint/parser",
	"plugins": [
		"@typescript-eslint"
	],
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/recommended"
	],
	"rules": {
		"prefer-const": "off",
		"semi": ["error", "always"],
		"quotes": ["error", "double"],
		"@typescript-eslint/no-unused-vars": "off"
	}
}